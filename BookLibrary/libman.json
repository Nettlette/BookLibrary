{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/js/jquery/"
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-select@1.13.18",
      "destination": "wwwroot/bootstrap-select/"
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@3.8.2",
      "destination": "wwwroot/chartjs"
    },
    {
      "provider": "cdnjs",
      "library": "date-fns@2.0.0",
      "destination": "wwwroot/date-fns"
    }
  ]
}